<div>
<a href="/">back to index</a>
</div>

<div>
<a href="/actors/add">add actors</a>
</div>

<table>
  <tr>
    <th>Picture</th>
    <th>Name</th>
    <th>Bio</th>
    <th>DOB</th>
    <th>Actions</th>
    <th></th>
  </tr>
  <% @list_of_actors.each do |actor| %>
  <tr>
    <td>
      <img src="<%= actor.image_url %>" width="80">
    </td>
    <td>
      <%= actor.name %>
    </td>
    <td>
      <%= actor.bio %>
    </td>
    <td>
      <%= actor.dob %>
    </td>
    <td>
      <a href="http://localhost:3000/actors/<%= actor.id %>/edit">Edit</a>
    </td>
    <td>
      <a href="http://localhost:3000/actors/<%= actor.id %>/delete">delete</a>
    </td>
  </tr>
  <tr>
    <td><br> </td>
  </tr>
  <% end %>
</table>
